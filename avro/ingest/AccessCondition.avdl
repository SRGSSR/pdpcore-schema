@namespace("ch.srgssr.pdp.domain.ingest")
protocol AccessCondition {

    /**
Assets with the access condition "Internal" must be made available only to authenticated "SRG SSR" employees or services.
Assets with the access condition "Public" can be made available to everyone.
If you are unsure set the access condition to "Internal" and/or contact the PDP team for clarification.
    */
    enum AccessCondition {
        Internal,
        Public
    }

}
